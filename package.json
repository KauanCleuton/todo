    "cors": "^2.8.5",
    "express": "^4.18.2",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/node": "^20.8.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.5",
    "autoprefixer": "^10",
    "concurrently": "^8.2.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "postcss": "^8",
    "prisma": "^5.4.1",
